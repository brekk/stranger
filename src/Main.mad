import IO from "IO"
import Wish from "Wish"

import LCG from "./LCG"



main = () => {
  IO.putLine("Hello World!")
  a = LCG.randomSeed()
  b = LCG.randomSeed()
  c = map(LCG.next)(a)
  pipe(
    Wish.parallel,
    Wish.fulfill((x) => { IO.trace("bad", x) }, (x) => { IO.trace("good", x) }),
  )([a, b, c])
  // map(IO.trace("mathy math"))(LCG.randomInt(0, 100))
  // map(IO.trace("seedy seeds"))(LCG.randomSeed())
}
